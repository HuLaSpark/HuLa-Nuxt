{
  "name": "hula-nuxt",
  "private": true,
  "type": "module",
  "version": "v1.1.0",
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=8.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HuLaSpark/HuLa-Nuxt.git"
  },
  "bugs": {
    "url": "https://github.com/HuLaSpark/HuLa-Nuxt/issues"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --config .prettierrc --write",
      "eslint src"
    ]
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "semantic-release": "dotenv -c -- semantic-release",
    "semantic-release:local": "dotenv -c -- semantic-release --no-ci"
  },
  "dependencies": {
    "@nuxt/image": "^1.8.1",
    "@nuxt/ui": "^2.18.6",
    "@nuxtjs/i18n": "^8.5.5",
    "nuxt": "^3.13.0",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@iconify-json/solar": "^1.2.1",
    "@release-it/conventional-changelog": "^9.0.0",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "^7.15.0",
    "conventional-changelog-cmyr-config": "^2.1.2",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.27.0",
    "lint-staged": "^15.2.10",
    "only-allow": "^1.2.1",
    "prettier": "^3.3.3",
    "sass": "^1.79.4",
    "semantic-release": "^24.1.2"
  }
}
